<mujoco>
	<visual>
		<!-- <global realtime="0.2"/> -->
		<headlight ambient="0.5 0.5 0.5" />
		<rgba haze="0.15 0.25 0.35 1"/>
	</visual>

	<option timestep="0.001" integrator="Euler" gravity="0 0 -9.8" cone="elliptic" solver="PGS">
		<flag energy="enable" contact="enable" />
	</option>

    <asset>
	    <texture type="skybox" builtin="gradient" rgb1=".3 .5 .7" rgb2="0 0 0" width="32" height="512"/>
	    <texture name="body" type="cube" builtin="flat" mark="cross" width="127" height="1278" rgb1="0.8 0.6 0.4" rgb2="0.8 0.6 0.4" markrgb="1 1 1" random="0.01"/>
	    <material name="body" texture="body" texuniform="true" rgba="0.8 0.6 .4 1"/>
	    <texture name="grid" type="2d" builtin="checker" width="512" height="512" rgb1=".1 .2 .3" rgb2=".2 .3 .4"/>
	    <material name="grid" texture="grid" texrepeat="1 1" texuniform="true" reflectance=".2"/>
  	</asset>

  	<default>
  		<!-- <joint damping=".2" stiffness="1" armature=".01" limited="true" solimplimit="0 .99 .01"/> -->
  		<joint damping=".04"/>
  	</default>

	<worldbody>
		<!-- <light diffuse="1.5 1.5 1.5" pos="0 0 3" dir="0 0 -1" ambient="0.5 0.5 0.5"/> -->
		<geom type="plane" size="5 5 0.1"  pos="0 0 0" contype="1" material="grid"/> <!--rgba=".9 0 0 1"-->
		<body name="link0" pos="0 0 1.11" euler="0 0 0">
			<joint name="body" type="free"/>
			<!-- <joint name="body1" type="hinge" axis="0 1 0"/>
			<joint name="body2" type="slide" axis="1 0 0"/>
			<joint name="body3" type="slide" axis="0 0 1"/> -->
			<!-- <site name="cot" pos="0 0 -0.1" size="0.03"/> -->
			<!-- <inertial pos="0 0 0" mass="1" diaginertia="0.01 0.01 0.01"/> -->
			<!-- <joint name="x" type="slide" pos="0 0 0.01" axis="1 0 0" />
			<joint name="y" type="slide" pos="0 0 0.01" axis="0 1 0" />
			<joint name="z" type="slide" pos="0 0 0.01" axis="0 0 1" /> -->
			<geom type="box" size=".08 .20 .1" rgba="0.7 .4 1 1" mass="10" pos="0 0 0"/>  <!--rgba=".9 0 0 1"-->
			<!-- <geom type="box" size=".1 .1 .1" rgba=".9 0 0 1" mass="10" pos="0 -0.2 -0.3"/>
			<geom type="box" size=".1 .1 .1" rgba=".9 0 0 1" mass="10" pos="0 0.2 -0.3"/> -->
			<body name="link1" pos="0 -0.15 -0.1" euler="0 0 0">
				<joint name="HIPRY" type="hinge" axis = "0 1 0" pos="0 0 0" limited="true" range="-120 120"/>
				<joint name="HIPRX" type="hinge" axis = "1 0 0" pos="0 0 0" limited="true" range="-90 90"/>
				<joint name="HIPRZ" type="hinge" axis = "0 0 1" pos="0 0 0" limited="true" range="-90 90"/>
				<geom type="cylinder" size="0.04 0.25" rgba="0 .9 0.5 1" mass="3" pos="0 0 -0.25"/>

				<body name="link2" pos="0 0 -0.5" euler="0 0 0">
					<joint name="KNEERY" type="hinge" axis = "0 1 0" pos="0 0 0" limited="true" range="0 170"/>
					<geom type="cylinder" size="0.04 0.2" rgba="0 .9 0 1" mass="3" pos="0 0 -0.2" contype="0"/> <!--size="0.02 0.25" pos="0 0 -0.25-->

					<body name="link3" pos="0 0 -0.5" euler="0 0 0">
						<joint name="ANKLERY" type="hinge" axis = "0 1 0" pos="0 0 0" limited="true" range="-75 60"/>
						<!-- <site name="sr" pos="0 0 0" size="0.03"/> -->
						<joint name="ANKLERX" type="hinge" axis = "1 0 0" pos="0 0 0" limited="true" range="-45 45"/>
						<geom type="box" size="0.13 0.04 0.01" rgba="0 0 .9 1" mass="0.5" pos="0.06 0 0" condim="4" friction="1000 200 200" />
						<!-- <geom type="cylinder" size="0.01 0.03" rgba="0 0.2 .9 1" mass="0.1" pos="0.2 0 0" euler="90 0 0" condim="4" friction="6 6 6" /> -->
						<site name="t1" pos="0.19 0.038 -0.01" size="0.004"/>
						<site name="t2" pos="-0.07 0.038 -0.01" size="0.004"/>
						<site name="t3" pos="0.19 -0.038 -0.01" size="0.004"/>
						<site name="t4" pos="-0.07 -0.038 -0.01" size="0.004"/>
					</body>
				</body>
			</body>

			<body name="link4" pos="0 0.15 -0.1" euler="0 0 0">
				<joint name="HIPLY" type="hinge" axis = "0 1 0" pos="0 0 0" limited="true" range="-120 120"/>
				<joint name="HIPLX" type="hinge" axis = "1 0 0" pos="0 0 0" limited="true" range="-90 90"/>
				<joint name="HIPLZ" type="hinge" axis = "0 0 1" pos="0 0 0" limited="true" range="-90 90"/>
				<geom type="cylinder" size="0.04 0.25" rgba="0 .9 0.5 1" mass="3" pos="0 0 -0.25"/>

				<body name="link5" pos="0 0 -0.5" euler="0 0 0">
					<joint name="KNEELY" type="hinge" axis = "0 1 0" pos="0 0 0" limited="true" range="0 170"/>
					<geom type="cylinder" size="0.04 0.2" rgba="0 .9 0 1" mass="3" pos="0 0 -0.2" contype="0"/>

					<body name="link6" pos="0 0 -0.5" euler="0 0 0">
						<joint name="ANKLELY" type="hinge" axis = "0 1 0" pos="0 0 0" limited="true" range="-75 60"/>
						<!-- <site name="sl" pos="0 0 0" size="0.03"/> -->
						<joint name="ANKLELX" type="hinge" axis = "1 0 0" pos="0 0 0" limited="true" range="-45 45"/>
						<geom type="box" size="0.13 0.04 0.01" rgba="0 0 .9 1" mass="0.5" pos="0.06 0 0" condim="4" friction="1000 200 200" />
						<!-- <geom type="cylinder" size="0.01 0.03" rgba="0 0.2 .9 1" mass="0.1" pos="0.2 0 0" euler="90 0 0" condim="4" friction="6 6 6" /> -->
						<site name="t5" pos="0.19 0.038 -0.01" size="0.004"/>
						<site name="t6" pos="-0.07 0.038 -0.01" size="0.004"/>
						<site name="t7" pos="0.19 -0.038 -0.01" size="0.004"/>
						<site name="t8" pos="-0.07 -0.038 -0.01" size="0.004"/>
					</body>
				</body>
			</body>

            <body name="link7" pos="0 0 0.1" euler="0 0 0">
                <joint name="WAISTY" type="hinge" axis = "0 1 0" pos="0 0 0" limited="true" range="-80 80"/>
				<joint name="WAISTX" type="hinge" axis = "1 0 0" pos="0 0 0" limited="true" range="-70 70"/>
                <geom type="box" size=".08 .25 .35" rgba="0.3 .3 1 1" mass="20" pos="0 0 0.35"/>
                <!-- <body name="link8" pos="0 0 0.3" euler="0 0 0">
                    <joint name="WAIST2Y" type="hinge" axis = "0 1 0" pos="0 0 0" limited="true" range="-80 80"/>
    				<joint name="WAIST2X" type="hinge" axis = "1 0 0" pos="0 0 0" limited="true" range="-70 70"/>
                    <geom type="box" size=".08 .25 .3" rgba="0.3 .3 1 1" mass="20" pos="0 0 0.3"/>
                </body> -->
            </body>
		</body>
	</worldbody>
	<actuator>
		<motor name="torque1"  joint="HIPRY" 	gear="1" ctrlrange="-1800 1800" ctrllimited="true"/>
		<motor name="torque2"  joint="HIPRX" 	gear="1" ctrlrange="-1800 1800" ctrllimited="true"/>
		<motor name="torque3"  joint="HIPRZ" 	gear="1" ctrlrange="-1100 1100" ctrllimited="true"/>
		<motor name="torque4"  joint="KNEERY" 	gear="1" ctrlrange="-1400 1400" ctrllimited="true"/>
		<motor name="torque5"  joint="ANKLERY" 	gear="1" ctrlrange="-1200 1200" ctrllimited="true"/>
		<motor name="torque6"  joint="ANKLERX" 	gear="1" ctrlrange="-1200 1200" ctrllimited="true"/>
		<motor name="torque7"  joint="HIPLY" 	gear="1" ctrlrange="-1800 1800" ctrllimited="true"/>
		<motor name="torque8"  joint="HIPLX" 	gear="1" ctrlrange="-1800 1800" ctrllimited="true"/>
		<motor name="torque9"  joint="HIPLZ" 	gear="1" ctrlrange="-1100 1100" ctrllimited="true"/>
		<motor name="torque10" joint="KNEELY" 	gear="1" ctrlrange="-1400 1400" ctrllimited="true"/>
		<motor name="torque11" joint="ANKLELY" 	gear="1" ctrlrange="-1200 1200" ctrllimited="true"/>
		<motor name="torque12" joint="ANKLELX" 	gear="1" ctrlrange="-1200 1200" ctrllimited="true"/>
        <motor name="torque13" joint="WAISTY" 	gear="1" ctrlrange="-1200 1200" ctrllimited="true"/>
		<motor name="torque14" joint="WAISTX" 	gear="1" ctrlrange="-1200 1200" ctrllimited="true"/>
        <!-- <motor name="torque1"  joint="HIPRY" 	gear="1" ctrlrange="-800 800" ctrllimited="true"/>
		<motor name="torque2"  joint="HIPRX" 	gear="1" ctrlrange="-800 800" ctrllimited="true"/>
		<motor name="torque3"  joint="HIPRZ" 	gear="1" ctrlrange="-100 100" ctrllimited="true"/>
		<motor name="torque4"  joint="KNEERY" 	gear="1" ctrlrange="-400 400" ctrllimited="true"/>
		<motor name="torque5"  joint="ANKLERY" 	gear="1" ctrlrange="-200 200" ctrllimited="true"/>
		<motor name="torque6"  joint="ANKLERX" 	gear="1" ctrlrange="-200 200" ctrllimited="true"/>
		<motor name="torque7"  joint="HIPLY" 	gear="1" ctrlrange="-800 800" ctrllimited="true"/>
		<motor name="torque8"  joint="HIPLX" 	gear="1" ctrlrange="-800 800" ctrllimited="true"/>
		<motor name="torque9"  joint="HIPLZ" 	gear="1" ctrlrange="-100 100" ctrllimited="true"/>
		<motor name="torque10" joint="KNEELY" 	gear="1" ctrlrange="-400 400" ctrllimited="true"/>
		<motor name="torque11" joint="ANKLELY" 	gear="1" ctrlrange="-200 200" ctrllimited="true"/>
		<motor name="torque12" joint="ANKLELX" 	gear="1" ctrlrange="-200 200" ctrllimited="true"/> -->
	</actuator>
	<sensor>
		<touch site="t1" />
		<touch site="t2" />
		<touch site="t3" />
		<touch site="t4" />
		<touch site="t5" />
		<touch site="t6" />
		<touch site="t7" />
		<touch site="t8" />
	</sensor>
</mujoco>